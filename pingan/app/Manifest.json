{
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "manifest_version": 2,
  "name": "Performance Monitor",
  "version": "1.0",
  "description": "A performance monitor to show cpu and memory status.",
  "permissions": [
    "system.cpu",
    "system.memory",
    "webview",
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage"
  ]
}